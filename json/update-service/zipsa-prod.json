{
    "cluster": "zipsa-production",
    "desiredCount": 2,
    "taskDefinition": "zipsa-prod",
    "capacityProviderStrategy": [
        {
            "capacityProvider": "Infra-ECS-Cluster-zipsa-production-0c9fa807-EC2CapacityProvider-9zKu6RFwdUrc",
            "weight": 1,
            "base": 1
        }
    ],
    "deploymentConfiguration": {
